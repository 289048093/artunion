<decorators defaultdir="/decorators">
	<decorator name="common" page="common.jsp">
		<pattern>/*.jsp</pattern>
	</decorator>
	<decorator name="extjs" page="extjsDecorator.jsp">
		<pattern>/userCenter/*.jsp</pattern>
	</decorator>
	<excludes>
		<pattern>/*_.jsp*</pattern>
		<pattern>/login.jsp*</pattern>
		<pattern>/jump.jsp*</pattern>
		<pattern>/error/*.jsp*</pattern>
	</excludes>
</decorators>